{
  "manifest_version": 3,
  "name": "Proxy Rotator Toggle",
  "version": "1.0",
  "description": "Rotate proxies with a toggle button",
  "permissions": [
    "proxy",
    "storage",
    "tabs",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Proxy Rotator"
  }
}
